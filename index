<script>
// Loading for the bottom parallax
$(window).bind("load", function() {
  setTimeout(function(){ $(window).trigger('resize.px.parallax'); }, 500);

});

$("#aboutBut").click(function() {
    $('html, body').animate({
        scrollTop: $("#about").offset().top  - 50
    }, 500);
});

$("#workBut").click(function() {
    $('html, body').animate({
        scrollTop: $("#work").offset().top  - 50
    }, 500);
});
$("#codeBut").click(function() {
    $('html, body').animate({
        scrollTop: $("#code").offset().top  - 50
    }, 500);
});
$("#portfolioBut").click(function() {
    $('html, body').animate({
        scrollTop: $("#portfolio").offset().top - 50
    }, 500);
});
$("#contactBut").click(function() {
    $('html, body').animate({
        scrollTop: $("#contact").offset().top - 50
    }, 500);
});

